<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tabnumber1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >
<ScrollView 	
				android:id="@+id/ScrollView1"
				android:layout_width="fill_parent" 
				android:layout_height="wrap_content"
				android:scrollbars="vertical">
		<LinearLayout
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:orientation="vertical" >
			    <TextView android:layout_width="fill_parent"
			    android:layout_height="wrap_content"
			    android:text="搜索结果:"
			    android:textSize="25sp"
			    android:gravity="center_horizontal"
			    android:layout_marginTop="10dp" />
			    <TextView android:layout_width="fill_parent"
			        android:layout_height="wrap_content"
			        android:id="@+id/main_activity_aboveleft_result"
			        android:textColor="@color/zise"
			        android:textSize="20sp"
			        android:gravity="center_horizontal"/>
			    <LinearLayout 
			        android:layout_width="fill_parent"
			        android:layout_height="wrap_content"
			        android:orientation="horizontal">
			    <TextView android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:text="企业名称:"
			        android:textSize="20sp"
			        android:textStyle="bold"
			        android:background="#dddddd"/>
			    <TextView android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:textSize="18sp"
			        android:id="@+id/main_activity_aboveleft_result_name"/>
			    </LinearLayout>
			    <LinearLayout 
			        android:layout_width="fill_parent"
			        android:layout_height="wrap_content"
			        android:orientation="horizontal">
			     <TextView android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:text="企业注册号:"
			        android:textSize="20sp"
			        android:textStyle="bold"
			        android:background="#dddddd"/>
			    <TextView android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:textSize="18sp"
			        android:id="@+id/main_activity_aboveleft_result_number"/>
			    </LinearLayout>
			    <LinearLayout 
			        android:layout_width="fill_parent"
			        android:layout_height="wrap_content"
			        android:orientation="horizontal">
			    <TextView android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:text="企业类型:"
			        android:textSize="20sp"
			        android:textStyle="bold"
			        android:background="#dddddd"/>
			    <TextView android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:textSize="18sp"
			        android:id="@+id/main_activity_aboveleft_result_type"/>
			    </LinearLayout>
			    <LinearLayout 
			        android:layout_width="fill_parent"
			        android:layout_height="wrap_content"
			        android:orientation="horizontal">
			    <TextView android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:text="投资人:"
			        android:textSize="20sp"
			        android:textStyle="bold"
			        android:background="#dddddd"/>
			    <TextView android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:textSize="18sp"
			        android:id="@+id/main_activity_aboveleft_result_people"/>
			    </LinearLayout>
			    <TextView android:layout_width="fill_parent"
			        android:layout_height="wrap_content"
			        android:text="企业地址:"
			        android:textSize="20sp"
			        android:textStyle="bold"
			        android:background="#dddddd"/>
			    <TextView android:layout_width="fill_parent"
			        android:layout_height="wrap_content"
			        android:textSize="18sp"
			        android:id="@+id/main_activity_aboveleft_result_address"/>
			    <TextView android:layout_width="fill_parent"
			        android:layout_height="wrap_content"
			        android:text="经营范围:"
			        android:textSize="20sp"
			        android:textStyle="bold"
			        android:background="#dddddd"/>
			    <TextView android:layout_width="fill_parent"
			        android:layout_height="wrap_content"
			        android:textSize="18sp"
			        android:id="@+id/main_activity_aboveleft_result_scope"/>
			    <TextView android:layout_width="fill_parent"
			        android:layout_height="wrap_content"
			        android:text="登记机关:"
			        android:textSize="20sp"
			        android:textStyle="bold"
			        android:background="#dddddd"/>
			    <TextView android:layout_width="fill_parent"
			        android:layout_height="wrap_content"
			        android:textSize="18sp"
			        android:id="@+id/main_activity_aboveleft_result_gear"/>
			    <LinearLayout 
			        android:layout_width="fill_parent"
			        android:layout_height="wrap_content"
			        android:orientation="horizontal">
			    <TextView android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:text="发证日期:"
			        android:textSize="20sp"
			        android:textStyle="bold"
			        android:background="#dddddd"/>
			    <TextView android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:textSize="18sp"
			        android:id="@+id/main_activity_aboveleft_result_issuedate"/>
			    </LinearLayout>
			    <LinearLayout 
			        android:layout_width="fill_parent"
			        android:layout_height="wrap_content"
			        android:orientation="horizontal">
			    <TextView android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:text="成立日期:"
			        android:textSize="20sp"
			        android:textStyle="bold"
			        android:background="#dddddd"/>
			    <TextView android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:textSize="18sp"
			        android:id="@+id/main_activity_aboveleft_result_builddate"/>
			    </LinearLayout>
			     <LinearLayout 
			        android:layout_width="fill_parent"
			        android:layout_height="wrap_content"
			        android:orientation="horizontal">
			    <TextView android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:text="经营状态:"
			        android:textSize="20sp"
			        android:textStyle="bold"
			        android:background="#dddddd"/>
			    <TextView android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:textSize="18sp"
			        android:id="@+id/main_activity_aboveleft_result_status"/>
			    </LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
		    